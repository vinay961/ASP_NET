@model IEnumerable<EntityFramework.Models.Student>

<table border="1">
    <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>AGE</th>
        <th>GENDER</th>
    </tr>

    @{ 
        foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Gender</td>
            </tr>
        }
    }

</table>